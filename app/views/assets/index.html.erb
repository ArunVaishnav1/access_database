<p id="notice"><%= notice %></p>

<h1>Assets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assets.each do |asset| %>
      <tr>
        <td><%= asset[:"Asset No"] %></td>
        <td><%= asset[:"Asset Category"] %></td>
        <td><%= asset[:Model]%></td>
        <td><%= asset[:Description]%></td>
        <td><%= asset[:Owner]%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
